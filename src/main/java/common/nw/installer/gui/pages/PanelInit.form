<?xml version="1.0" encoding="UTF-8"?>
<form xmlns="http://www.intellij.com/uidesigner/form/" version="1" bind-to-class="common.nw.installer.gui.pages.PanelInit">
  <grid id="27dc6" binding="panel_init" layout-manager="GridBagLayout">
    <constraints>
      <xy x="20" y="20" width="500" height="400"/>
    </constraints>
    <properties/>
    <border type="empty">
      <size top="5" left="5" bottom="5" right="5"/>
    </border>
    <children>
      <vspacer id="1a2ad">
        <constraints>
          <grid row="3" column="0" row-span="1" col-span="2" vsize-policy="6" hsize-policy="1" anchor="0" fill="2" indent="0" use-parent-layout="false"/>
          <gridbag weightx="0.0" weighty="1.0"/>
        </constraints>
      </vspacer>
      <component id="15d47" class="javax.swing.JLabel">
        <constraints>
          <grid row="1" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="0" anchor="8" fill="0" indent="0" use-parent-layout="false"/>
          <gridbag top="0" left="0" bottom="0" right="4" weightx="0.0" weighty="0.0"/>
        </constraints>
        <properties>
          <labelFor value="e28c6"/>
          <text value="Modpack Url:"/>
        </properties>
      </component>
      <component id="e28c6" class="javax.swing.JTextField" binding="txtUrl">
