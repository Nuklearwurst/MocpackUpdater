<?xml version="1.0" encoding="UTF-8"?>
<form xmlns="http://www.intellij.com/uidesigner/form/" version="1" bind-to-class="common.nw.installer.gui.PanelFinish">
  <grid id="27dc6" binding="panel_finish" layout-manager="GridBagLayout">
    <constraints>
      <xy x="20" y="20" width="500" height="400"/>
    </constraints>
    <properties/>
    <border type="none"/>
    <children>
      <component id="7a1d8" class="javax.swing.JTextPane" binding="txtpnInstructions">
        <constraints>
          <grid row="1" column="0" row-span="1" col-span="1" vsize-policy="6" hsize-policy="6" anchor="0" fill="3" indent="0" use-parent-layout="false">
            <preferred-size width="150" height="50"/>
          </grid>
          <gridbag weightx="0.0" weighty="0.0"/>
        </constraints>
        <properties>
          <editable value="false"/>
          <font name="Arial" size="12"/>
          <text value="To use the modpack select the versionname you chose in the profile editor under 'use Version'&#10;&#10;You might also want to change the Game-Directory and  JVM-arguments (for bigger modpacks)"/>
        </properties>
      </component>
      <component id="ae28a" class="javax.swing.JLabel">
        <constraints>
          <grid row="2" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="0" anchor="8" fill="0" indent="0" use-parent-layout="false"/>
          <gridbag top="4" left="0" bottom="0" right="0" weightx="0.0" weighty="0.0"/>
        </constraints>
        <properties>
          <text value="Example JVM arguments:"/>
        </properties>
      </component>
      <component id="846a3" class="javax.swing.JTextField" binding="txtExampleJVMOptions">
        <constraints>
          <grid row="3" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="6" anchor="8" fill="1" indent="0" use-parent-layout="false">
            <preferred-size width="150" height="-1"/>
          </grid>
          <gridbag top="4" left="4" bottom="4" right="4" weightx="0.0" weighty="0.0"/>
        </constraints>
        <properties>
          <editable value="false"/>
          <text value="-Xmx2G -XX:PermSize=256m -XX:MaxPermSize=512m"/>
        </properties>
      </component>
      <scrollpane id="9119c">
        <constraints>
          <grid row="0" column="0" row-span="1" col-span="1" vsize-policy="7" hsize-policy="7" anchor="0" fill="3" indent="0" use-parent-layout="false"/>
          <gridbag weightx="1.0" weighty="1.0"/>
        </constraints>
        <properties/>
        <border type="none"/>
        <children>
          <component id="3d57f" class="javax.swing.JTextPane" binding="txtpnFinish">
            <constraints/>
            <properties>
              <editable value="false"/>
              <font name="Arial" size="12" style="3"/>
              <text value="Installation Finished!"/>
            </properties>
          </component>
        </children>
      </scrollpane>
    </children>
  </grid>
</form>
